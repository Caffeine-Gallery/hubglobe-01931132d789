type NewsArticle = 
 record {
   content: text;
   continent: text;
   date: int;
   id: text;
   title: text;
 };
type Hub = 
 record {
   contact: text;
   continent: text;
   description: text;
   id: text;
   location: text;
   name: text;
   website: text;
 };
service : {
  getAllContinents: () -> (vec text) query;
  getHubsByContinent: (text) -> (vec Hub) query;
  getNewsByContinent: (text) -> (vec NewsArticle) query;
}
